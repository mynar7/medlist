{
  "name": "medlist",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "16.15.1"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently --kill-others \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mynar7/medlist.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mynar7/medlist/issues"
  },
  "homepage": "https://github.com/mynar7/medlist#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "body-parser": "^1.20.1",
    "cookie-session": "^2.0.0-beta.3",
    "dotenv": "^6.0.0",
    "express": "^4.18.2",
    "if-env": "^1.0.4",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "qr-image": "^3.2.0",
    "sequelize": "^4.38.0"
  },
  "devDependencies": {
    "concurrently": "^2.2.0",
    "nodemon": "^1.17.5"
  }
}
